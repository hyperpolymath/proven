{
  "name": "@proven/rescript-bindings",
  "version": "0.1.0",
  "exports": {
    ".": "./src/ProvenResult.res",
    "./SafeUrl": "./src/ProvenSafeUrl.res",
    "./SafeString": "./src/ProvenSafeString.res",
    "./SafeJson": "./src/ProvenSafeJson.res"
  },
  "tasks": {
    "build": "deno run -A npm:rescript",
    "clean": "deno run -A npm:rescript clean",
    "watch": "deno run -A npm:rescript -w"
  },
  "imports": {
    "rescript": "npm:rescript@^12.1.0",
    "@rescript/core": "npm:@rescript/core@^1.7.0",
    "proven/": "../../javascript/src/"
  },
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false
  }
}
